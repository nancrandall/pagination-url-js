{
  "name": "pagination-poc",
  "description": "Base layout for pagination to be integrated into JKP and AEM projects",
  "version": "1.0.16",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open",
    "build": "cross-env NODE_ENV=production webpack --progress --verbose",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov mocha",
    "showcoverage": "open coverage/lcov-report/index.html",
    "push": "./push",
    "sass": "node sass.js",
    "sass-watch": "nodemon -e .scss sass.js",
    "copy-assets": "node copyAssets.js"
  },
  "dependencies": {},
  "devDependencies": {
    "ajv": "^6.5.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^6.0.0",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "chai-dom": "^1.8.1",
    "chai-jquery": "^2.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.0.0",
    "eslint": "^5.6.0",
    "eslint-loader": "^2.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.9.0",
    "fs-extra": "^2.0.0",
    "jsdom": "^13.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.2.0",
    "node-bourbon": "^4.2.8",
    "node-neat": "^1.7.2",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.3",
    "nyc": "^13.1.0",
    "styleguide": "~3.10.0",
    "stylelint": "^9.4.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "underscore": "^1.8.3",
    "underscore.string": "~3.0.1",
    "webpack": "^3.1.0",
    "webpack-dev-server": "^2.1.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
